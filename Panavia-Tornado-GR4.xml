          <property>metrics/Sw-sqft</property>
	  <property>aero/force/lift-coeff</property>
          <table>
            <independentVar lookup="row">aero/alpha-rad</independentVar>
            <tableData>
             -1.57    0.247
             -0.26    0.007
              0.00    0.003
              0.26    0.005
              1.57    0.050
            </tableData>
          </table>
       </product>
    </function>

    <function name="aero/force/Drag_induced">
